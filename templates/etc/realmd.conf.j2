[users]
 default-home = /home/%D/%U
 default-shell = /bin/bash

[active-directory]
 default-client = sssd
 os-name = {{ ansible_distribution }}
 os-version = {{ ansible_distribution_version }}

[service]
 automatic-install = no

[{{ sssd_kerberos_realm }}]
 fully-qualified-names = yes
 automatic-id-mapping = no
 user-principal = yes
 manage-system = yes